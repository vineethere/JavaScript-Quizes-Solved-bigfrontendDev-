// https://bigfrontend.dev/quiz/operator-precedence

console.log(0 == 1 == 2)
console.log(2 == 1 == 0)
console.log(0 < 1 < 2)
console.log(1 < 2 < 3)
console.log(2 > 1 > 0)
console.log(3 > 2 > 1)



// console.log(0 == 1 == 2)
// 0==1 false
// false==2
// 0==2 false
// console.log(2 == 1 == 0)
// 2==1. false
// false==0 
// 0 == 0 true

// console.log(2 > 1 > 0)
// 2>1 = true 
// true>0
// 1>0
// so true 
// console.log(3 > 2 > 1)
// 3>2 true
// true>1
// 1>1 false


